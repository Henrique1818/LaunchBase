{% extends "layout.njk" %}

{% block content %}
    <div class="cards">
        <table>
            <thead>
                <th>Instrutores</th>
                <th>Atendimentos</th>
            </thead>
            <tbody>
                {%for instructor in instructors %}
                    <tr>
                        <td>{{instructor.name}}</td>
                        <th>{{instructor.services}}</th>
                    </tr>
                {%endfor%}
            </tbody>
        </table>
    </div>
{% endblock content %}